{
  "name": "libre-weather",
  "version": "0.1.0",
  "private": true,
  "license": "AGPL-3.0",
  "dependencies": {
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@parcel/packager-raw-url": "^2.8.0",
    "@parcel/transformer-less": "^2.8.0",
    "@parcel/transformer-webmanifest": "^2.8.0",
    "@svgr/parcel-plugin-svgr": "^5.5.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^5.0.0",
    "dotenv": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jquery": "^3.6.1",
    "less": "^3.12.2",
    "license-list": "^0.1.3",
    "nominatim-browser": "^2.1.0",
    "parcel": "^2.8.0",
    "popper.js": "^1.16.1",
    "prettier": "^2.0.5",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-animated-weather": "^4.0.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.7"
  },
  "scripts": {
    "start": "parcel ./src/index.html --no-cache",
    "build": "npm run build:licenses && parcel build ./src/index.html --no-cache",
    "lint": "eslint src/.",
    "lint:fix": "eslint src/. --fix",
    "serve": "serve -p $PORT -s dist/.",
    "test": "jest",
    "coverage": "jest --coverage",
    "build:licenses": "node src/utilities/generateLicenses.js",
    "build:docker": "docker build -t libreweather/frontend:latest ."
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": true,
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "printWidth": 120
  },
  "eslintIgnore": [
    ".github/*",
    "dist/*",
    "node_modules/*",
    "app.json",
    "package.json",
    "package-lock.json"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "window": "readonly",
      "localStorage": "readonly",
      "navigator": "readonly"
    },
    "rules": {
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": false,
          "requireReturnDescription": false,
          "preferType": {
            "String": "string",
            "Number": "number",
            "Boolean": "boolean",
            "Function": "function",
            "object": "Object",
            "date": "Date",
            "error": "Error"
          },
          "prefer": {
            "return": "returns"
          }
        }
      ],
      "no-underscore-dangle": "off",
      "strict": [
        "error",
        "safe"
      ],
      "linebreak-style": "off",
      "no-restricted-syntax": "off",
      "no-await-in-loop": "off",
      "global-require": "off",
      "no-fallthrough": "off",
      "no-param-reassign": "off",
      "no-case-declarations": "off",
      "no-nested-ternary": "off",
      "react/prefer-stateless-function": "off",
      "react/prop-types": [
        0
      ],
      "default-case": "off",
      "react/jsx-closing-bracket-location": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "max-classes-per-file": "off",
      "react/jsx-one-expression-per-line": "off"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "babelOptions": {
        "plugins": [
          "@babel/plugin-transform-react-jsx"
        ]
      }
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@components",
              "./src/components"
            ]
          ],
          "extensions": [
            ".js",
            ".json",
            ".node",
            ".jsx"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "jkuester",
  "contributors": [
    "tobitenno"
  ],
  "engines": {
    "node": "16.x",
    "npm": ">=8.11.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "alias": {
    "@/common/*": "./src/components/common/$1",
    "@components/*": "./src/components/$1"
  }
}
